root ::= "{" space x-kv "," space y-kv "," space z-kv "}" space
space ::= | " " | "\n" [ \t]{0,20}
x ::= ([1-9] | ([1-8] [0-9] | [9] [0-9]) | "1" ([0-4] [0-9] | [5] "0")) space
x-kv ::= "\"x\"" space ":" space x
y ::= ([1-9] | ([1-8] [0-9] | [9] [0-9]) | "1" ([0-4] [0-9] | [5] "0")) space
y-kv ::= "\"y\"" space ":" space y
z ::= ([1-9] | ([1-8] [0-9] | [9] [0-9]) | "1" ([0-4] [0-9] | [5] "0")) space
z-kv ::= "\"z\"" space ":" space z
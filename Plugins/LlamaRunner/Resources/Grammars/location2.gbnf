root ::= "{" space x-kv "," space y-kv "," space z-kv "}" space
space ::= | " " | "\n" [ \t]{0,20}
x ::= (([5-8] [0-9]{2} | [9] ([0-8] [0-9] | [9] [0-9])) | ([1-4] [0-9]{3} | [5] "000")) space
x-kv ::= "\"x\"" space ":" space x
y ::= (([5-8] [0-9]{2} | [9] ([0-8] [0-9] | [9] [0-9])) | ([1-4] [0-9]{3} | [5] "000")) space
y-kv ::= "\"y\"" space ":" space y
z ::= (([5-8] [0-9]{2} | [9] ([0-8] [0-9] | [9] [0-9])) | ([1-4] [0-9]{3} | [5] "000")) space
z-kv ::= "\"z\"" space ":" space z